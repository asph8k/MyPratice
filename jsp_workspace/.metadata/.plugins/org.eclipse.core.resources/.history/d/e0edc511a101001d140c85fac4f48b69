package prefix_sum;

import java.util.Scanner;

public class 구간_합_구하기02_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt(), M = sc.nextInt();
		int[][] A = new int[N][N];
		
		//원본 배열 A 채우기
		for(int i = 0; i < N; i++) {
			for(int j = 0; j < N; j++) {
				A[i][j] = sc.nextInt();
			}
		}
	}

}
