package bubble;

public class BubbleSort {

	//버블 정렬
	public static void sort(int[] arr) {
		//비교 범위를 잡아주는 반복문
		for(int i = arr.length-1; i > 0; i--) {
			//실제 비교를 통한 정렬
			boolean flag = false;
			for(int j = 0; j < i; j++) {
				
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {33, 11, 99, 1, 22, 88, 55, 44, 66, 77};
		sort(arr);
	}

}
