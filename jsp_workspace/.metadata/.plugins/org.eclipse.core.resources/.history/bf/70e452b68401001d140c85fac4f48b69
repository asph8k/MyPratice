		int N = sc.nextInt(), M = sc.nextInt();
		long[] A = new long[N];
		
		for(int i = 0; i < N; i++) {
			A[i] = sc.nextInt();
		}
		
		//합배열 S를 만들자
		//S[i] = S[i-1] + A[i]
	}

}
